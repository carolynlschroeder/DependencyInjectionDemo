@model List<DependencyInjectionDemo.Entities.Staff>

@{
    ViewBag.Title = "Staff List";
}
<h2>@ViewBag.Title</h2>

<table class="table table-striped">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
    </tr>
    @foreach (var m in Model)
    {
        <tr>
            <td>@m.FirstName</td>
            <td>@m.LastName</td>
            <td>@m.Email</td>
        </tr>
    }
</table>